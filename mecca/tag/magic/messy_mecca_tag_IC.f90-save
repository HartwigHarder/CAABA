      ! ----- NC4H10 -----
      IDO(tag_IC_NC4H10,2) = ( &
        - F13C(tag_IC_NC4H10) * ( flow(NC4H10_sink) ) )

      ! ----- LC4H9O2 -----
      IDO(tag_IC_LC4H9O2,2) = ( &
        - F13C(tag_IC_LC4H9O2) * ( flow(LC4H9O2_sink) )  &
