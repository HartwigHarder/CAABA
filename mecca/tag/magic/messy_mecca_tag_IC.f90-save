  ELEMENTAL REAL(dp) FUNCTION zerodiv(what,by)
    ! safe division which gives zero when division by zero is performed
    REAL(dp), INTENT(IN) :: what, by   ! operands
    IF (by .EQ. 0.0_dp) THEN
      zerodiv = 0.0_dp
    ELSE
      zerodiv = what/by
    ENDIF
  END FUNCTION zerodiv



