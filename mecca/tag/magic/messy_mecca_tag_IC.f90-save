      ! ----- ISOOH -----
      IDO(tag_IC_ISOOH,2) = ( &
        - F13C(tag_IC_ISOOH) * ( flow(ISOOH_sink) )  &
        +  F13C(tag_IC_ISO2) * ( flow(ISO2_ISOOH) ) )

      ! ----- ISON -----
      IDO(tag_IC_ISON,2) = ( &
        -  F13C(tag_IC_ISON) * ( flow(ISON_sink) )  &
        +  F13C(tag_IC_C5H8) * ( flow(C5H8_ISON) )  &
        +  F13C(tag_IC_ISO2) * ( flow(ISO2_ISON) ) )

    END SUBROUTINE iderivs

